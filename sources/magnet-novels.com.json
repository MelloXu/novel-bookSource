{
  "name": "MAGNET MACROLINK",
  "url": "magnet-novels.com",
  "version": 100,
  "search": {
      "url": "https://www.magnet-novels.com/search?q=${key}",
      "charset": "utf-8",
      "list": "$.data.data[*]",
      "name": "$.data.data[*].name",
      "author": "$.data.data[*].author.name",
      "detail": "https://www.magnet-novels.com/novels/${$.data.data[*].id}",
      "summary": "$.data.data[*].synopsis"
  },
  "detail": {
    "name": "$.data.name",
    "author": "$.data.author.name",
    "summary": "$.data.synopsis",
    "cover": "$.data.cover_url",
    "update": "$.data.last_section.latest_public_time",
    "lastChapter": "$.data.last_section.title"
  },
  "catalog": {
      "list": "$.data[*]",
      "orderBy": 0,
      "name": "$.data[*].title",
      "chapter": ".book_list li a"
  },
  "chapter": {
      "content": ".read_chapterDetail"
  },
  "rank": [
      {
          "title": "ジャンル",
          "url": "https://www.magnet-novels.com/genre/${key}",
          "page": 1,
          "size": 10,
          "categories": [{"key": "fantasy", "value": "ファンタジー"}, {"key": "love", "value": "恋愛"}],
          "list": "$.data.data[*]",
          "name": "$.data.data[*].name",
          "author": "$.data.data[*].author.name",
          "cover": "$.data.data[*].cover_url",
          "detail": "https://www.magnet-novels.com/novels/${$.data.data[*].id}"
      }
  ]
}