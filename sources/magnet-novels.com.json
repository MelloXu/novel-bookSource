{
  "name": "MAGNET MACROLINK",
  "url": "magnet-novels.com",
  "version": 100,
  "search": {
      "url": "https://www.magnet-novels.com/api/novel/searchNovels@post->keyword=${key}&size=20&order_by='latest_update_time'&sort='desc'",
      "charset": "utf-8",
      "list": "$.data.data[*]",
      "name": "$.data.data[*].name",
      "author": "$.data.data[*].author.name",
      "detail": "/novels/${$.data.data[*].id}",
      "summary": "$.data.data[*].synopsis"
  },
  "detail": {
    "name": "$.data.name",
    "author": "$.data.author.name",
    "summary": "$.data.synopsis",
    "cover": "$.data.cover_url",
    "update": "$.data.last_section.latest_public_time",
    "lastChapter": "$.data.last_section.title"
  },
  "catalog": {
      "list": "$.data[*]",
      "orderBy": 0,
      "name": "$.data[*].title",
      "chapter": ".book_list li a"
  },
  "chapter": {
      "content": ".read_chapterDetail"
  },
  "rank": [
      {
          "title": "ファンタジー",
          "url": "https://www.magnet-novels.com/api/web/v2/get24hHotNovels@post->genre=1&page=1",
          "page": 1,
          "size": 10,
          "list": "$.data.data[*]",
          "name": "$.name",
          "author": "$.author.name",
          "cover": "$.cover_url",
          "detail": "https://www.magnet-novels.com/novels/${$.data.data[*].id}"
      }
  ]
}